<?xml version='1.' encoding='utf-8'?>
<widget id="zw.co.flashtech.talentlashes" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Talent Lashes</name>
    <description>
        An application for Talent register her lash extension appointments
    </description>
    <author email="innocentmazando@gmail.com" href="http://flashtech.co.zw">
        Innocent Mazando
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <icon src="img/icon.png"/>
</widget>
