{{#if this}}
    <div class="list-block media-list searchbar-found" id="dates_list"
         style="margin-top: 0px; position: relative; z-index: 2;">
        <ul>
            {{#each this}}
                <li data-id="{{_id}}" class="result-item {{checkdate _date}}">

                    <a href="pages/appointment.html?id={{_id}}" class="item-link">
                        <div class="item-content">
                            <div class="item-media">
                                <img src="img/lashes/lash1.jpg" width="40" class="lazy">
                            </div>
                            <div class="item-inner">
                                <div class="item-title-row">
                                    <div class="item-title">
                                        <b style="color: #2c28c5;">
                                            {{name}}
                                        </b>
                                    </div>
                                </div>
                                <div class="item-subtitle"
                                     style="text-overflow: ellipsis; white-space: nowrap; overflow: hidden;">

                                    <div class="chip">
                                        <div class="chip-label">
                                            {{ _time }}
                                        </div>
                                    </div>

                                    {{changedate _date }}


                                </div>
                                <!-- <div id="underliner"></div>-->
                            </div>
                        </div>
                    </a>
                </li>
            {{/each}}
        </ul>
    </div>

{{else}}
    <h4>No results</h4>
{{/if}}